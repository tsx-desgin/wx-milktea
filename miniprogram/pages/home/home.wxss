.swiper-container{
  width: 100%;
  height: 400rpx;
}
.swiper-img{
  width: 100%;
  height: 100%;
}
.navigate{
  width: 100%;
  height: 400rpx;
  margin-top: 50rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  background: #fff;
}
.navigate-list{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
}
.navigate-cell{
  width: 33.33%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.navigate-cell::before{
  content: '';
  display: block;
  width: 1px;
  height:140rpx;
  background: #333;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}
.navigate-cell:nth-child(3n+1)::before{
  display: none;
}
.navigate-img{
  width: 80rpx;
  height: 80rpx;
}
.navigate-name{
  font-size: 20rpx;
  margin-top: 20rpx;
}
.banner-container{
  width: 100%;
  height: 300rpx;
  margin: 50rpx 0;
}
.banner-img{
  width: 100%;
  height: 100%;
}